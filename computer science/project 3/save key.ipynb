    "def savekey(dic, name, extrabits):\n",
    "    dic.update({'extrabits': extrabits})\n",
    "    print(dic)\n",
    "    name = 'keyOf'+name.split('.')[0] + '.txt'\n",
    "    f = open(name, \"w+\")\n",
    "    f.write(str(dic))\n",
    "    f.close()\n",
    "    return"
